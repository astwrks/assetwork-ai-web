  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "./dev-quiet.sh",
    "dev:clean": "node start-clean.js",
    "dev:verbose": "next dev --turbopack -p 3001",
    "dev:no-turbopack": "next dev -p 3001",
    "build": "npm run prisma:clean && prisma generate && next build",
    "analyze": "ANALYZE=true npm run build",
    "start": "next start -p 3002",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next out node_modules/.cache",
    "postinstall": "npm run prisma:clean && prisma generate",
    "prisma:clean": "rm -rf node_modules/.prisma node_modules/@prisma/client/.prisma || true",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:studio": "prisma studio",
    "db:migrate": "prisma migrate dev",
    "db:test": "tsx scripts/test-neon-connection.ts",
    "test": "jest"
  },
